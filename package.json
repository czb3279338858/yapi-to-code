{
  "name": "yapi-to-code",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "bin": {
    "yapi-to-code": "dist/bin/yapi-to-code.umd.js"
  },
  "main": "./dist/yapi-to-code.umd.cjs",
  "module": "./dist/yapi-to-code.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/yapi-to-code.mjs",
    "require": "./dist/yapi-to-code.umd.cjs"
  },
  "scripts": {
    "build-lib": "vite build --config dist.vite.config.ts",
    "build-bin": "vite build --config bin.vite.config.ts",
    "build": "npm run build-lib && npm run build-bin",
    "release": "npm run build && npm version patch && npm publish",
    "dev": "ts-node lib/bind/yapi-to-code.ts --ids 94299",
    "build-dev":"npm run build && node dist/bin/yapi-to-code.umd.js --ids 94299"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.12.7",
    "json-schema": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "commander": "^12.0.0",
    "json-schema-to-typescript": "^14.0.0"
  }
}
